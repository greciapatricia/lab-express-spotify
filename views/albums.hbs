<div>
    <div class="row">
        {{#each items as | item |}}
        {{#with item}}
        <div class="col-lg-4 col-md-6">
            <div class="card bg-dark" style="width: 18rem;">
                <img src="{{image_url}}" class="card-img-top" alt="...">
                <div class="card-body d-flex flex-column justify-content-center align-items-center ">
                    <p class="text-white text-center fs-4 text-truncate" style="max-width: 200px">{{name}}</p>
                    <a href="/tracks/{{id}}" class="btn btn-danger">View Tracks</a>
                </div>
            </div>
        </div>
        {{/with}}
        {{/each}}

    </div>
</div>